  {{!-- datatable --}}
  <script>
    $(document).ready(function () {
      $('#example').DataTable({
        
        "bLengthChange": false,
        

        pagingType: "full_numbers",


      });
    });
  </script>



  {{#if formValidate}}
  <script>
    $(document).ready(function () {
      jQuery.validator.addMethod("noSpace", (value, element) => {
        return value.indexOf(' ') < 0 && value != '';
      }, "Please avoid spaces")
      jQuery.validator.addMethod("notNumber", function (value, element, param) {
        var reg = /[0-9]/;
        if (reg.test(value)) {
          return false;
        } else {
          return true;
        }
      }, "Number is not permitted");

      $("#formdsub").validate({

        rules: {
          name: {
            required: true,
            maxlength: 30,
            minlength: 3,
            digits: false,
            notNumber: true
          },
          password: {
            required: true,
            minlength: 3,
            maxlength: 10,
            noSpace: true
          },
          confirm_password: {
            required: true,
            equalTo: '#password',
            noSpace: true
          },
          email: {
            required: true,
            email: true,
            noSpace: true
          },
          phone: {
            required: true,
            minlength: 10,
            maxlength: 10,
            digits: true,
            noSpace: true
          },
          address: {
            required: true,
            minlength: 5
          },
          description: {
            required: true
          },
          price: {
            required: true,
            digits: true
          },
          count: {
            required: true
          },
          category1: {
            required: true
          },
          image: {
            required: true,
            accept: "image/jpg,image/jpeg,image/png,image/gif"
          },
          category: {
            required: true
          },
          Subcategory: {
            required: true
          },
          code: {
            required: true,
            minlength: 4,
            maxlength: 6,
            digits: true

          },
          firstname: {
            required: true,
            minlength: 2,
            maxlength: 25,
            digits: false,
            notNumber: true
          },
          lastname: {
            required: true,
            minlength: 1,
            maxlength: 25,
            digits: false,
            notNumber: true
          },
          pincode: {
            required: true,
            digits: true,
            minlength: 6,
            maxlength: 6
          },
          payment: {
            required: true
          },
          phonenumber: {
            required: true,
            minlength: 10,
            maxlength: 10,
            digits: true,
            noSpace: true
          },
          min: {
            required: true

          },
          max: {
            required: true
          }

        },
        errorClass: "invalid"
      })

    })

  </script>
  {{/if}}
